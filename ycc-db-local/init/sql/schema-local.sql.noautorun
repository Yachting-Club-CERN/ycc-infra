  	FOR rec IN curs LOOP
  		IF infoyear='ALL' THEN
  			counter := counter + 1;
  			htp.TableRowOpen;
  			htp.TableData(counter);
  			htp.TableData(rec.name || ' ' || rec.firstname);
  			htp.TableRowClose;
  		ELSIF infoyear='NEW' THEN
  			IF rec.member_entrance = query_year THEN
  				counter := counter + 1;
  				htp.TableRowOpen;
  				htp.TableData(counter);
  				htp.TableData(rec.name || ' ' || rec.firstname);
  				htp.TableRowClose;
  			END IF;
  		ELSE
