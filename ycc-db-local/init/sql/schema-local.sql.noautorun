/*********************************/

	PROCEDURE MembershipList (query_year IN VARCHAR2)
  IS
    	CURSOR m_curs IS
  		SELECT name, firstname, membership, lang1
  		FROM members, feesrecords
  		WHERE id=member_id AND year_f = query_year
